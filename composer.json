{
    "name": "{{NAME}}",
    "repositories": {
        "0": {
            "type": "git",
            "url": "https://git.oschina.net/zan-group/zanphp.git"
        },
        "1": {
            "type": "git",
            "url": "https://git.oschina.net/zan-group/nova-generic.git"
        },
        "2": {
            "type": "git",
            "url": "https://git.oschina.net/zan-group/nova.git"
        },
        "3": {
            "type": "path",
            "url": "nova-sdk/nova-demo/",
            "options": {
                "symlink": false
            }
        },
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    },
    "require": {
        "zanphp/zan": "dev-master",
        "nova-service/nova-demo": "*"
    },
    "autoload": {
        "psr-4": {
            "{{NAMESPACE}}": "src",
            "{{NAMESPACE}}Init\\": "init"
        }
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    },
    "prefer-stable": false,
    "minimum-stability": "dev"
}

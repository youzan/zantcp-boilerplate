{
    "name": "{{NAME}}",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/youzan/zanphp.git"
        },
        {
            "type": "path",
            "url": "nova-sdk/nova-demo/",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "zanphp/zan": "dev-master",
        "nova-service/nova-demo": "*"
    },
    "autoload": {
        "psr-4": {
            "{{NAMESPACE}}": "src",
            "{{NAMESPACE}}Init\\": "init"
        }
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    },
    "prefer-stable": false,
    "minimum-stability": "dev"
}
